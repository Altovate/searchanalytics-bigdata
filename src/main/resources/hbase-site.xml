	</property>
	<property>
		<name>hbase.rootdir</name>
		<value>hdfs://localhost.localdomain:54321/hbase</value>
	</property>
	<property>
		<name>hbase.master.port</name>
		<value>44335</value>
	</property>
	<property>
		<name>hbase.master.info.port</name>
		<value>44345</value>
	</property>
	<property>
		<name>hbase.regionserver.port</name>
		<value>44435</value>
	</property>
	<property>
		<name>hbase.regionserver.info.port</name>
		<value>44445</value>
	</property>
	<property>
		<name>zookeeper.znode.parent</name>
		<value>/hbase</value>
	</property>
	<property>
		<name>hbase.master.distributed.log.splitting</name>
		<value>false</value>
	</property>
	<property>
		<name>hbase.master.distributed.log.replay</name>
		<value>false</value>
